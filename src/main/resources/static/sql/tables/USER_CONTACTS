CREATE TABLE `contactmanager`.`user_contact` (
  `USER_CONTACT_ID` INT NOT NULL,
  `NAME` VARCHAR(245) NULL,
  `SECOND_NAME` VARCHAR(245) NULL,
  `WORK` VARCHAR(245) NULL,
  `EMAIL` VARCHAR(245) NULL,
  `PHONE` VARCHAR(45) NULL,
  `IMAGE` VARCHAR(245) NULL,
  `DESCRIPTION` VARCHAR(1000) NULL,
  PRIMARY KEY (`USER_CONTACT_ID`));
